# Poetry plugin for Oh My Zsh
# Opinionated list of poetry commands, only the things I use frequently

# Aliases
alias poe='poetry'
alias poi='poetry install'
alias por='poetry run'
alias pot='poetry run pytest -s'
alias poad='poetry add'
alias poadd='poetry add --dev'
alias porm='poetry remove'
alias mkserve='poetry run mkdocs serve'
